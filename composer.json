  "require": {
    "php": "^7.0",
    "segura/appcore": "*",
    "segura/sessions": "*",
    "slim/slim": "^3.2",
    "php-school/cli-menu": "^2.0.0",
    "kint-php/kint": "^2.0",
    "zendframework/zend-db": "^2.7",
    "zeuxisoo/slim-whoops": "^0.5.0",
    "symfony/var-dumper": "^3.0",
    "slim/twig-view": "^2.1",
    "twig/extensions": "^1.3",
    "mattketmo/camel": "^1.1",
    "gone.io/uuid": "^2.2",
    "gone.io/inflection": "^2.0",
    "monolog/monolog": "^1.19.0",
    "predis/predis": "^1.0",
    "jimmiw/php-time-ago": "^0.4.11",
    "sebastian/diff": "^1.4",
    "guzzlehttp/guzzle": "^6.2.1",
    "facebook/webdriver": "^1.2",
    "squizlabs/php_codesniffer": "^2.7",
    "friendsofphp/php-cs-fixer": "^1.12",
    "johnkary/phpunit-speedtrap": "~1.0@dev",
    "lstrojny/phpunit-clever-and-smart": "0.*",
    "brianium/paratest": "^0.14.0",
    "fzaninotto/Faker": "^1.6.0",
    "phpunit/phpunit": "5.*",
    "deweller/cliopts": "^1.0",
    "gone.io/twig-extension-transform": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Zenderator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zenderator\\Test\\": "tests/"
    }
  }
}
