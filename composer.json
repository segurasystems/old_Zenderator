{
  "name": "segura/zenderator",
  "authors": [
    {
      "name": "Matthew Baggett",
      "email": "matthew@baggett.me"
    }
  ],
  "bin": [
    "generator/automize",
    "generator/phpcsfixerfier",
    "generator/psr2ifier",
    "generator/sdkifier",
    "generator/zenderator"
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:segurasystems/LibAppCore.git"
    }
  ],
  "require": {
    "php": "^7.0",
    "segura/appcore": "*",
    "segura/sessions": "*",
    "slim/slim": "^3.2",
    "php-school/cli-menu": "^2.0.0",
    "raveren/kint": "^1.0",
    "zendframework/zend-db": "^2.7",
    "zeuxisoo/slim-whoops": "^0.5.0",
    "symfony/var-dumper": "^3.0",
    "slim/twig-view": "^2.1",
    "twig/extensions": "^1.3",
    "mattketmo/camel": "^1.1",
    "thru.io/uuid": "^2.0",
    "thru.io/inflection": "^1.0",
    "monolog/monolog": "^1.19.0",
    "predis/predis": "^1.0",
    "jimmiw/php-time-ago": "^0.4.11",
    "sebastian/diff": "^1.4",
    "guzzlehttp/guzzle": "^6.2.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "dev-master",
    "friendsofphp/php-cs-fixer": "^2.0@dev",
    "fzaninotto/Faker": "dev-master",
    "phpunit/phpunit": "5.*"
  },
  "autoload": {
    "psr-4": {
      "Zenderator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zenderator\\Test\\": "tests/"
    }
  }
}
