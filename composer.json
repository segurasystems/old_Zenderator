{
  "name": "segura/zenderator",
  "authors": [
    {
      "name": "Matthew Baggett",
      "email": "matt.baggett@segura.co.uk"
    }
  ],
  "bin": [
    "generator/clean",
    "generator/phpcsfixerfier",
    "generator/sdkifier",
    "generator/zenderator"
